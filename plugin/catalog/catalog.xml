<?xml version="1.0" encoding="UTF-8"?>
<installation type="plugin">
	<name>catalog</name>	
	<text>Wtyczka prezentacji dokumentów w danej kategorii</text>
	<author>Adam Boduch</author>
	<version>1.0-dev</version>
	<system>1.0-rc1</system>
	
	<option type="select">
		<name>enableCatalog</name>
		<text>Wyświetlaj katalog</text>
		<title>Prezentacja podstron w formie katalogu</title>
		<values default="0">
			<value id="0">Wyłączona</value>
			<value id="1">Włączona</value>
		</values>
		<auth>a_</auth>
	</option>
</installation>